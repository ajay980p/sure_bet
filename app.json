{
    "expo": {
        "name": "sure_bet_calculator",
        "slug": "sure_bet_calculator",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.webp",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.webp",
                "backgroundColor": "#ffffff"
            },
            "package": "com.ajay980p.sure_bet_calculator"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/splash-icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "17371777-a7ee-4257-b91d-72ddbc295058"
            }
        }
    }
}